{
  "name": "cimon-desktop",
  "productName": "cimon-desktop",
  "version": "1.0.29",
  "description": "desktop client for ci monitoring tool",
  "main": "./dist/main/index.js",
  "keywords": [],
  "author": {
    "name": "Vadym Artemchuk",
    "email": "vadimart92@gmail.com"
  },
  "license": "MIT",
  "homepage": "https://github.com/ci-mon/cimon",
  "build": {
    "appId": "com.squirrel.cimon_desktop.cimon-desktop",
    "notifier_exe_name": "cimon-desktop-notifier.exe"
  },
  "type": "module",
  "scripts": {
    "format": "prettier --write .",
    "lint": "eslint . --ext .js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts,.vue --fix",
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
    "typecheck:web": "vue-tsc --noEmit -p tsconfig.web.json --composite false",
    "typecheck": "npm run typecheck:node && npm run typecheck:web",
    "start": "electron-vite preview",
    "dev": "electron-vite dev --watch",
    "package": "npm run build && electron-forge package",
    "make": "npm run build && electron-forge make",
    "make:mac": "npm run build && electron-forge make -- --platform=darwin",
    "publish": "npm i && npm run build && electron-forge publish",
    "publish:mac": "npm i && npm run build && electron-forge publish -- --platform=darwin",
    "build": "npm run typecheck && electron-vite build",
    "debugPublish": "electron-forge publish"
  },
  "dependencies": {
    "eventsource": "^2.0.2",
    "fetch-cookie": "^2.2.0",
    "node-fetch": "^2.7.0",
    "tough-cookie": "^4.1.3",
    "vue": "^3.4.35",
    "vue-router": "^4.4.2",
    "vuetify": "^3.6.14",
    "ws": "^8.16.0"
  },
  "devDependencies": {
    "@electron-forge/cli": "^7.4.0",
    "@electron-forge/maker-deb": "^7.4.0",
    "@electron-forge/maker-rpm": "^7.4.0",
    "@electron-forge/maker-squirrel": "^7.4.0",
    "@electron-forge/maker-zip": "^7.4.0",
    "@electron-forge/shared-types": "^7.4.0",
    "@electron-toolkit/eslint-config": "^1.0.2",
    "@electron-toolkit/eslint-config-ts": "^1.0.1",
    "@electron-toolkit/preload": "^3.0.0",
    "@electron-toolkit/tsconfig": "^1.0.1",
    "@electron-toolkit/utils": "^3.0.0",
    "@mdi/font": "^7.4.47",
    "@microsoft/signalr": "^8.0.0",
    "@rushstack/eslint-patch": "^1.8.0",
    "@types/changelog-parser": "^2.8.4",
    "@types/debug": "^4.1.12",
    "@types/electron-squirrel-startup": "^1.0.2",
    "@types/node": "^20.11.30",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/eslint-config-typescript": "^13.0.0",
    "auto-launch": "^5.0.6",
    "changelog-parser": "^3.0.1",
    "electron": "^31.3.1",
    "electron-is-dev": "^3.0.1",
    "electron-log": "^5.1.2",
    "electron-request": "^1.8.3",
    "electron-squirrel-startup": "^1.0.0",
    "electron-store": "^8.2.0",
    "electron-updater": "^6.1.8",
    "electron-vite": "^2.2.0",
    "eslint": "^8.57.0",
    "eslint-plugin-vue": "^9.25.0",
    "form-data": "^4.0.0",
    "node-win-toast-notifier": "^0.1.17",
    "nodejs-gravatar": "^1.0.2",
    "prettier": "^3.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.4",
    "vite": "^5.3.5",
    "vue-eslint-parser": "^9.4.2",
    "vue-tsc": "^2.0.29"
  }
}
