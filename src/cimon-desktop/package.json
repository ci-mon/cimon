{
  "name": "cimon-desktop",
  "productName": "cimon-desktop",
  "version": "1.0.10",
  "description": "desktop client for ci monitoring tool",
  "main": "./dist/main/index.js",
  "keywords": [],
  "author": {
    "name": "Vadym Artemchuk",
    "email": "vadimart92@gmail.com"
  },
  "license": "MIT",
  "homepage": "https://github.com/ci-mon/cimon",
  "build": {
    "appId": "com.squirrel.cimon_desktop.cimon-desktop",
    "notifier_exe_name": "cimon-desktop-notifier.exe"
  },
  "type": "commonjs",
  "scripts": {
    "format": "prettier --write .",
    "lint": "eslint . --ext .js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts,.vue --fix",
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
    "typecheck:web": "vue-tsc --noEmit -p tsconfig.web.json --composite false",
    "typecheck": "npm run typecheck:node && npm run typecheck:web",
    "start": "electron-vite preview --outDir=dist",
    "dev": "electron-vite dev --outDir=dist --watch",
    "package": "electron-vite build --outDir=dist && electron-forge package",
    "make": "electron-vite build --outDir=dist && electron-forge make",
    "make:mac": "electron-vite build --outDir=dist && electron-forge make -- --platform=darwin",
    "publish": "electron-vite build --outDir=dist && electron-forge publish",
    "build": "npm run typecheck && electron-vite build",
    "postinstall": "electron-builder install-app-deps"
  },
  "dependencies": {
    "@electron-toolkit/preload": "^3.0.0",
    "@electron-toolkit/utils": "^3.0.0",
    "electron-updater": "^6.1.7"
  },
  "devDependencies": {
    "@electron-toolkit/eslint-config": "^1.0.2",
    "@electron-toolkit/eslint-config-ts": "^1.0.1",
    "@electron-toolkit/tsconfig": "^1.0.1",
    "@rushstack/eslint-patch": "^1.7.1",
    "@types/node": "^18.19.9",
    "@vitejs/plugin-vue": "^5.0.3",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "electron": "^28.2.0",
    "electron-builder": "^24.9.1",
    "electron-vite": "^2.0.0",
    "eslint": "^8.56.0",
    "eslint-plugin-vue": "^9.20.1",
    "prettier": "^3.2.4",
    "typescript": "^5.3.3",
    "vite": "^5.0.12",
    "vue": "^3.4.15",
    "vue-tsc": "^1.8.27",
    "vue-eslint-parser": "^9.4.2",
    "vue-router": "^4.2.5",
    "vuetify": "^3.5.1",
    "@electron-forge/cli": "^7.2.0",
    "@electron-forge/maker-deb": "^7.2.0",
    "@electron-forge/maker-rpm": "^7.2.0",
    "@electron-forge/maker-squirrel": "^7.2.0",
    "@electron-forge/maker-zip": "^7.2.0",
    "@electron-forge/shared-types": "^7.2.0",
    "@microsoft/signalr": "^8.0.0",
    "electron-is-dev": "^3.0.1",
    "electron-log": "^5.1.0",
    "electron-request": "^1.8.3",
    "electron-squirrel-startup": "^1.0.0",
    "node-win-toast-notifier": "^0.1.14",
    "nodejs-gravatar": "^1.0.2",
    "@mdi/font": "^7.4.47",
    "@types/changelog-parser": "^2.8.4",
    "@types/debug": "^4.1.12",
    "@types/electron-squirrel-startup": "^1.0.2",
    "changelog-parser": "^3.0.1",
    "node-fetch": "^3.3.2"
  }
}
