{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "DbProvider": "Sqlite",
  "ConnectionStrings": {
    "Sqlite": "Data Source=./db/data.db;",
    "SqlServer": "Data Source=.;Initial Catalog=cimon;Integrated Security=True;"
  },
  "Secrets": {
    "Vault": {
      "Url": "",
      "Token": "root",
      "MountPoint": "infrastructure.cimon",
      "Path": "prod"
    },
    "LdapClient": {
      "ConnectionTimeout": "00:00:10",
      "TeamGroups": ["DevTeams"],
      "AdminGroups": ["Admins"]
    }
  },
  "AllowedHosts": "*",
  "Authentication": {
    "Negotiate": {
      "Enabled": true
    }
  }
}
